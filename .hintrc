{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scroll-timeline-name",
          "scroll-timeline-axis"
        ]
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 130",
    "not chrome <= 109",
    "not firefox <= 132",
    "not ios_saf <= 18.1",
    "not safari <= 18.1"
  ]
}